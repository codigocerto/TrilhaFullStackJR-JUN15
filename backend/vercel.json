{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "app.py"
      },
      {
        "src": "/projetos",
        "dest": "app.py"
      },
      {
        "src": "/projeto/criar",
        "dest": "app.py"
      },
      {
        "src": "/projetos/criar",
        "dest": "app.py"
      },
      {
        "src": "/projeto/editar",
        "dest": "app.py"
      },
      {
        "src": "/projeto/deletar",
        "dest": "app.py"
      },
      {
        "src": "/projetos/deletar",
        "dest": "app.py"
      }

    ]
  }